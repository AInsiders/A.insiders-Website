{
  "name": "ainsiders-website",
  "private": true,
  "version": "1.0.0",
  "description": "Official website for A.Insiders - AI and cybersecurity solutions",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
